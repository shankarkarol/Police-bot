{
  "name": "police-form-automation-microservice",
  "version": "1.0.0",
  "description": "Standalone microservice for Rajasthan Police form automation using Playwright",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test:deployment": "npx ts-node tests/deployment-test.ts",
    "test:deployment:prod": "npx ts-node tests/deployment-test.ts https://police-bot-production.up.railway.app",
    "test:deployment:local": "npx ts-node tests/deployment-test.ts http://localhost:3000"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.13",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "police-form",
    "automation",
    "playwright",
    "rajasthan-police",
    "microservice"
  ],
  "author": "shankarkarol",
  "license": "MIT"
}
